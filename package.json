{
  "name": "mlbb-sdk",
  "version": "1.0.4",
  "description": "TypeScript SDK for MLBB Stats API",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    },
    "./enums": {
      "import": "./dist/enums.js",
      "types": "./dist/enums.d.ts"
    }
  },
  "scripts": {
    "dev": "vite-node src/MlbbAPI.ts",
    "generate": "vite-node scripts/generate-index.ts",
    "clean": "rimraf dist",
    "test": "vite-node tests/test.ts",
    "build:js": "tsc -p tsconfig.build.json",
    "build:dts": "tsc -p tsconfig.dts.json",
    "build": "npm run generate && npm run test && npm run clean && npm run build:js && npm run build:dts",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "mlbb",
    "sdk",
    "api",
    "typescript"
  ],
  "author": "Твоё имя",
  "license": "MIT",
  "dependencies": {
    "ofetch": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^24.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.6.0",
    "vite-node": "^3.2.4"
  }
}
